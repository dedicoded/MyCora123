{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true pnpm run build",
    "audit": "pnpm audit --audit-level=high && npx size-limit && node scripts/env-check.js",
    "audit:full": "pnpm audit && pnpm run perf:audit && pnpm run size-check",
    "audit:security": "pnpm audit --audit-level=high",
    "build": "next build",
    "clean": "rm -rf node_modules .next && pnpm install",
    "clean:cache": "rm -rf .next && pnpm run build",
    "console:clean": "node scripts/console-cleaner.js",
    "contracts:compile": "npx hardhat compile",
    "deploy:all": "npx hardhat run scripts/deploy-all-contracts.ts",
    "deploy:mainnet": "npx hardhat run scripts/deploy-all-contracts.ts --network mainnet",
    "deploy:sepolia": "npx hardhat run scripts/deploy-all-contracts.ts --network sepolia",
    "deployment:status": "cat DEPLOYMENT_STATUS_TRACKER.md",
    "detect-port": "node scripts/port-check.js",
    "predev": "node scripts/env-check.js",
    "dev": "next dev -p 5000 -H 0.0.0.0",
    "dev:clean": "pkill -f 'next dev' || true && fuser -k 3000/tcp 2>/dev/null || true && rm -rf .next && pnpm store prune && pnpm run dev",
    "dev:auto": "node scripts/port-check.js && pnpm run dev",
    "lint": "next lint",
    "perf:audit": "node scripts/performance-audit.js",
    "prepare": "echo 'Skipping prepare script in deployment'",
    "size-check": "npx size-limit",
    "start": "pnpm run validate:replit && pnpm run dev:clean",
    "restart": "pkill -f 'next dev' || true && pnpm run dev",
    "check-env": "node scripts/env-check.js",
    "test:contracts": "npx hardhat test",
    "test:deployment": "node scripts/test-deployment.js",
    "validate": "tsx scripts/production-validation.ts",
    "validate:compile": "tsc scripts/production-validation.ts && node scripts/production-validation.js",
    "validate:deployment": "node scripts/deployment-verification.js",
    "validate:env": "node scripts/env-check.js",
    "validate:replit": "node scripts/env-check.js",
    "validate:ui": "node scripts/deployment-ui-validation.js",
    "verify:all": "npx hardhat run scripts/verify-contracts.ts",
    "audit:dev": "pnpm audit --audit-level=high && pnpm run perf:audit",
    "audit:production": "echo 'Production audit skipped for faster deployment'",
    "deploy:replit": "pnpm run build && pnpm start",
    "sync:replit": "node scripts/sync-env-to-replit.js",
    "diagnose": "node scripts/component-diagnostic.js"
  },
